{
    "name": "count-character",
    "displayName": "Character Count in Selected Text",
    "description": "Gets character count of selected text",
    "version": "0.0.1",
    "publisher": "animesh",
    "engines": {
        "vscode": "^1.9"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:selectedText.characterCount"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "selectedText.characterCount",
            "title": "Count Characters (Selection)"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^5.4.5",
        "vscode": "^1.90.0",
        "mocha": "^10.4.0",
        "@types/node": "^20.14.2",
        "@types/mocha": "^10.0.6"
    }
}